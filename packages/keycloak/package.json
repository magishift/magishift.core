{
  "name": "@magishift/keycloak",
  "version": "0.0.1",
  "description": "Magishift is Low-Code Framework, helps you to build application easier, faster and a more efficient use of time. (@keycloak)",
  "author": "Sofyan Hadi Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magishift/magishift.core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "nodemon",
    "start:prod": "node dist/main.js",
    "sls:offline": " sls offline --port 3003 --skipCacheInvalidation",
    "build": "rm -rf dist && tsc --p tsconfig.json -d && copyfiles -u -f -V src/**/*.graphql src/**/**/*.graphql dist",
    "build:publish": "yarn build && yarn publish --no-git-tag-version",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "barrels": "barrelsby --delete -d ./src",
    "precommit": "yarn e2e:coverage",
    "e2e": "jest --config=jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=jest-e2e.json",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@hewmen/serverless-plugin-typescript": "^1.1.17",
    "@magishift/config": "^0.0.4",
    "@magishift/http": "^0.0.4",
    "@magishift/redis": "^0.0.3",
    "@magishift/util": "^0.0.3",
    "@nestjs/platform-express": "^6.1.1",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "dotenv": "^8.0.0",
    "ioredis": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "keycloak-admin": "^1.12.0",
    "moment": "^2.24.0",
    "pg": "^7.10.0",
    "reflect-metadata": "^0.1.13",
    "serverless-plugin-optimize": "^4.0.2-rc.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/swagger": "^3.0.2",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.25",
    "@types/aws-serverless-express": "^3.3.0",
    "@types/node": "^12.0.0",
    "@types/uuid": "^3.4.4",
    "barrelsby": "^2.0.0",
    "copyfiles": "^2.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^4.10.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
