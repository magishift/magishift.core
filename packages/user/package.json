{
  "name": "@magishift/user",
  "version": "0.0.0",
  "description": "Magishift is Low-Code Framework, helps you to build application easier, faster and a more efficient use of time. (@user)",
  "author": "Sofyan Hadi Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magishift/magishift.core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --p tsconfig.json -d && copyfiles -u -f -V src/**/*.graphql src/**/**/*.graphql dist",
    "precommit": "echo 'precommit'",
    "build:publish": "yarn build && yarn publish --no-git-tag-version",
    "barrels": "barrelsby --delete -d ./src"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@nestjs/graphql": "^6.2.4",
    "csvtojson": "^2.0.8",
    "lodash": "^4.17.11",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/uuid": "^3.4.4",
    "barrelsby": "^2.0.0",
    "copyfiles": "^2.1.0",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2",
    "typescript": "^3.4.5"
  }
}
