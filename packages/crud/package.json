{
  "name": "@magishift/crud",
  "version": "0.0.11",
  "description": "Magishift is Low-Code Framework, helps you to build application easier, faster and a more efficient use of time. (@crud)",
  "author": "Sofyan Hadi Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magishift/magishift.core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn barrels && rm -rf dist && tsc --p tsconfig.json -d && copyfiles -u -f -V src/**/*.graphql src/**/**/*.graphql dist",
    "precommit": "echo 'precommit'",
    "build:publish": "yarn build && yarn publish --no-git-tag-version",
    "barrels": "barrelsby --delete -d ./src --singleQuotes"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@magishift/base": "^0.0.4",
    "@magishift/util": "^0.0.3",
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/graphql": "^6.2.4",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.3.1",
    "@nestjs/typeorm": "^6.1.2",
    "apollo-server-express": "^2.6.2",
    "class-transformer": "^0.2.3",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.3.0",
    "json2csv": "^4.5.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.7",
    "@types/uuid": "^3.4.4",
    "barrelsby": "^2.1.1",
    "copyfiles": "^2.1.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "typescript": "^3.5.1"
  }
}
