{
  "name": "@magishift/crud",
  "version": "0.0.19",
  "description": "Magishift is Low-Code Framework, helps you to build application easier, faster and a more efficient use of time. (@crud)",
  "author": "Sofyan Hadi Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magishift/magishift.core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn barrels && rm -rf dist && tsc --p tsconfig.json -d",
    "precommit": "echo 'precommit'",
    "build:publish": "yarn build && yarn publish --no-git-tag-version",
    "barrels": "barrelsby --delete -d ./src --singleQuotes",
    "lint": "tslint --project .",
    "e2e": "jest --config=jest-e2e.json --forceExit --detectOpenHandles",
    "e2e:watch": "jest --watch --config=jest-e2e.json --detectOpenHandles",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage --detectOpenHandles"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@magishift/util": "^0.0.3",
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.3.1",
    "@nestjs/typeorm": "^6.1.2",
    "class-transformer": "^0.2.3",
    "json2csv": "^4.5.1",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/uuid": "^3.4.4",
    "barrelsby": "^2.1.1",
    "copyfiles": "^2.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "sqlite3": "^4.0.9",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  }
}
