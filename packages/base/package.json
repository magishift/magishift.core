{
  "name": "@magishift/base",
  "version": "0.0.3",
  "description": "Magishift is Low-Code Framework, helps you to build application easier, faster and a more efficient use of time. (@base)",
  "author": "Sofyan Hadi Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magishift/magishift.core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precommit": "echo 'precommit'",
    "build": "rm -rf dist && tsc --p tsconfig.json -d && copyfiles -u -f -V src/**/*.graphql src/**/**/*.graphql dist",
    "build:publish": "yarn build && yarn publish --no-git-tag-version ",
    "barrels": "barrelsby --delete -d ./src"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "class-validator": "^0.9.1",
    "os-utils": "^0.0.14",
    "type-graphql": "^0.17.3",
    "typeorm": "^0.2.17",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1"
  },
  "devDependencies": {
    "@types/node": "^11.13.9",
    "@types/uuid": "^3.4.4",
    "barrelsby": "^2.0.0",
    "copyfiles": "^2.1.0",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2",
    "typescript": "^3.4.5"
  }
}
