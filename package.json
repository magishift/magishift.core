    "start": "nodemon",
    "docker": "docker-compose up",
    "docker:down": "docker-compose down",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint --fix src/**/*.ts{,x}",
    "build": "rm -rf dist && tsc --p tsconfig.json -d && copyfiles -u -f -V src/**/*.graphql src/**/**/*.graphql dist",
    "precommit": "echo 'precommit'",
    "generate": "plop --plopfile internals/generators/index.ts",
    "test": "NODE_ENV=test jest --config=example/tests/jest.json --coverage",
    "test:watch": "NODE_ENV=test jest --watch --config=example/tests/jest.json",
    "test:coverage": "NODE_ENV=test jest --config=example/tests/jest.json --coverage --coverageDirectory=coverage",
    "e2e": "NODE_ENV=test jest --config=example/tests/jest-e2e.json --coverage --verbose --detectOpenHandles --forceExit --runInBand",
    "e2e:watch": "NODE_ENV=test jest --watch --config=example/tests/jest-e2e.json",
    "e2e:coverage": "NODE_ENV=test jest --config=example/tests/jest-e2e.json --coverage --coverageDirectory=coverage --detectOpenHandles --forceExit"
